<form action="job" method="post">
  Name: <input type="text" name="name"><br>
  Email: <input type="email" name="email"><br>
  Education:
  <select name="education">
    <option>SSLC</option><option>PUC</option><option>Degree</option>
  </select><br>
  Skills: <input type="text" name="skills"><br>
  Expected Salary: <input type="text" name="salary"><br>
  Experience:
  <select name="experience">
    <option>Fresher</option><option>1 Year</option><option>2+ Years</option>
  </select><br>
  <input type="submit" value="Submit">
</form>
