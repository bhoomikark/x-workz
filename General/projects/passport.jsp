<form action="passport" method="post">
  Applicant Name: <input type="text" name="name"><br>
  Aadhar No: <input type="text" name="aadhar"><br>
  Address: <input type="text" name="address"><br>
  PAN No: <input type="text" name="pan"><br>
  Country: <input type="text" name="country"><br>
  State: <input type="text" name="state"><br>
  City: <input type="text" name="city"><br>
  Pin Code: <input type="text" name="pin"><br>
  Passport Type:
  <select name="type">
    <option>Ordinary</option><option>Diplomatic</option><option>Official</option>
  </select><br>
  Payment Reference No: <input type="text" name="payment"><br>
  <input type="submit" value="Submit">
</form>
